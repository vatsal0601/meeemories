{
  "expo": {
    "name": "meeemories",
    "slug": "meeemories",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#030712"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vatsal0601.meeemories"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "monochromeImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#030712"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you add them as memories.",
          "cameraPermission": "The app accesses your camera to let you instantly add photos as memories."
        }
      ],
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "78d509ea-716b-4e2d-8e50-bebc67f2dc26"
      }
    },
    "experiments": {
      "typedRoutes": true
    },
    "owner": "vatsal0601",
    "scheme": "meeemories"
  },
  "scripts": {
    "tsc": "tsc"
  }
}
